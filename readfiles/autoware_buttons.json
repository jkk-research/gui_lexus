    },
    {
        "id":"usb_cam", "label":"usb_cam", "bgColor":"darkblue", "textColor":"lightblue", 
        "command":"'screen', '-mdS', 'usb_cam', 'source ~/autoware/install/setup.bash && ros2 launch lexus_bringup usb_web_cam1.launch.py" 
